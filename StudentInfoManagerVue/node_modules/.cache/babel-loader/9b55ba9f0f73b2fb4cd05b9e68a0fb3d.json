Name) {\n      this.$refs[formName].validate((valid) => {\n        if (valid) {\n          // 通过前端校验\n          const that = this\n          const sid = that.ruleForm.sid\n          const cid = that.ruleForm.cid\n          const tid = that.ruleForm.tid\n          const term = that.ruleForm.term\n          const grade = that.ruleForm.grade\n          axios.get(\"http://localhost:9451/SCT/updateById/\" + sid + '/' + cid + '/' + tid + '/' + term + '/' + grade).then(function (resp) {\n            if (resp.data === true) {\n              that.$message({\n                showClose: true,\n                message: '编辑成功',\n                type: 'success'\n              });\n            }\n            else {\n              that.$message.error('编辑失败，请检查数据库');\n            }\n            that.$router.push(\"/queryGradeCourse\")\n          })\n        } else {\n          return false;\n        }\n      });\n    },\n    resetForm(formName) {\n      this.$refs[formName].resetFields();\n    },\n    test() {\n      console.log(this.ruleForm)\n    }\n  }\n}\n</script>"],"mappings":";;AA0BA;EACAA,KAAA;IACA,IAAAC,UAAA,GAAAA,CAAAC,IAAA,EAAAC,KAAA,EAAAC,QAAA;MACA,KAAAD,KAAA;QACA,OAAAC,QAAA,KAAAC,KAAA;MACA;MACA,KAAAC,MAAA,CAAAC,SAAA,CAAAJ,KAAA;QACAC,QAAA,KAAAC,KAAA;MACA;QACA,IAAAF,KAAA,UAAAA,KAAA;UACAC,QAAA,KAAAC,KAAA;QACA;UACAD,QAAA;QACA;MACA;IACA;IACA;MACAI,QAAA;QACAC,GAAA;QACAC,KAAA;QACAC,KAAA;QACAC,GAAA;QACAC,KAAA;QACAC,GAAA;QACAC,KAAA;MACA;MACAC,KAAA;QACAL,KAAA,GACA;UAAAM,QAAA;UAAAC,OAAA;UAAAC,OAAA;QAAA,GACA;UAAAC,IAAA;UAAAF,OAAA;UAAAC,OAAA;QAAA,GACA;UAAAE,SAAA,EAAApB,UAAA;UAAAkB,OAAA;QAAA;MAEA;IACA;EACA;EACAG,QAAA;IACA,MAAAC,IAAA;IACA,KAAAf,QAAA,CAAAC,GAAA,QAAAe,MAAA,CAAAC,KAAA,CAAAhB,GAAA;IACA,KAAAD,QAAA,CAAAM,GAAA,QAAAU,MAAA,CAAAC,KAAA,CAAAX,GAAA;IACA,KAAAN,QAAA,CAAAI,GAAA,QAAAY,MAAA,CAAAC,KAAA,CAAAb,GAAA;IACA,KAAAJ,QAAA,CAAAkB,IAAA,QAAAF,MAAA,CAAAC,KAAA,CAAAC,IAAA;IACA,SAAAlB,QAAA,CAAAkB,IAAA;MACA,KAAAlB,QAAA,CAAAkB,IAAA;IACA;IACAC,KAAA,CAAAC,GAAA,yCACA,KAAApB,QAAA,CAAAI,GAAA,S